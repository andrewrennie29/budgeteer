$("ul.errors").html("")
<% if @expense.errors.any? %>
  <% @expense.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $("#expense-modal").modal("hide")
  $(".expenses-index").html("<%= escape_javascript(render 'index_table') %>")
  $(".expenses-chart").html("<%= escape_javascript(render 'index_chart') %>")
  $(".report-card-<%= @expense.budget.id %>").html("<%= escape_javascript(render 'reports/card', budget: @budget) %>")
  $(<% puts @expense.budget.name %>)
<% end %>