<p id="notice"><%= notice %></p>

<h1>Expense Owners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Allocation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expense_owners.each do |expense_owner| %>
      <tr>
        <td><%= expense_owner.name %></td>
        <td><%= expense_owner.allocation %></td>
        <td><%= link_to 'Show', expense_owner %></td>
        <td><%= link_to 'Edit', edit_expense_owner_path(expense_owner) %></td>
        <td><%= link_to 'Destroy', expense_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense Owner', new_expense_owner_path %>
